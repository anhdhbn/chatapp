<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root maxHeight="-Infinity" maxWidth="480.0" minHeight="-Infinity" minWidth="-Infinity" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="npclient.gui.controller.FileMessageController">
<!--<VBox maxHeight="-Infinity" maxWidth="480.0" minHeight="-Infinity" minWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">-->
   <children>
      <HBox styleClass="account" >
         <children>
            <ImageView fx:id="fileIcon" fitHeight="60.0" fitWidth="60.0" onMouseClicked="#onDownload" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/download.png" />
               </image>
            </ImageView>
            <VBox>
               <children>
                  <Text fx:id="tFileName" strokeType="OUTSIDE" strokeWidth="0.0" text="test.txt" />
                  <Text fx:id="tFileSize" strokeType="OUTSIDE" strokeWidth="0.0" text="150 KB" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
<!--</VBox>-->
</fx:root>
